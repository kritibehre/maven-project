pipeline 
{
	agent any
    stages
	{
		stage('SCM Checkout') 
		{
			steps 
				{
					git "https://github.com/kritibehre/maven-project.git"
				}
		}
		stage('Build')
       	{
			steps
				{
					withMaven(maven: 'LocalMaven')
					{
						sh 'mvn clean package install'
					}
				}
		}
        
	}
}		
