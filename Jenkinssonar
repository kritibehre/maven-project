pipeline 
{
	agent any
    stages
	{
		stage('SCM Checkout') 
		{
			steps 
				{
					git "https://github.com/kritibehre/maven-project.git"
				}
		}
		stage('Build')
       	{
			steps
				{
					withMaven(jdk: 'myjdk', maven: 'mymaven') 
					{
						sh "mvn clean install package"
					}
				}
		}
        
	}
}		
